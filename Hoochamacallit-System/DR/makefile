#
# =====================================================
#               Final binary Target
# =====================================================
./bin/DR : ./obj/DR.o
	cc ./obj/DR.o -g -o ./bin/DR

#
# =======================================================
#                  Dependencies
# =======================================================
./obj/DR.o : ./src/DR.c ../common/inc/common.h
	cc -c ./src/DR.c -o ./obj/DR.o

#
# =======================================================
#                  Other targets
# =======================================================
all : ./bin/DR

clean:
	rm -f ./bin/*
	rm -f ./obj/*.o
	